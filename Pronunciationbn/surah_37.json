        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3943,
        "text": "আফালা-তাযাক্কারূন।",
        "numberInSurah": 155,
        "juz": 23,
        "manzil": 6,
        "page": 452,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3944,
