        "juz": 23,
        "manzil": 6,
        "page": 450,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3899,
        "text": "ইন্নাহূমিন ‘ইবা-দিনাল মু’মুনীন।",
        "numberInSurah": 111,
        "juz": 23,
        "manzil": 6,
        "page": 450,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3900,
        "text": "ওয়া বাশশারনা-হু বিইছহা-কা নাবিইয়াম মিনাসসা-লিহীন।",
