  "code": 200,
  "status": "OK",
  "data": {
    "number": 37,
    "name": "سُورَةُ الصَّافَّاتِ",
    "englishName": "As-Saaffaat",
    "englishNameTranslation": "Those drawn up in Ranks",
    "revelationType": "Meccan",
    "numberOfAyahs": 182,
    "ayahs": [
      {
        "number": 3789,
        "text": "ওয়াসসাফফা-তি সাফফা-।",
        "numberInSurah": 1,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3790,
        "text": "ফাঝঝা-জিরা-তি ঝাজরা-।",
        "numberInSurah": 2,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3791,
        "text": "ফাত্তা-লিয়া-তি যিকরা-।",
        "numberInSurah": 3,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3792,
        "text": "ইন্না ইলা-হাকুম লাওয়া-হিদ।",
        "numberInSurah": 4,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3793,
        "text": "রাব্বুছ ছামা-ওয়া-তি ওয়াল আরদিওয়ামা-বাইনাহুমা-ওয়া রাব্বুল মাশা-রিক।",
        "numberInSurah": 5,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3794,
        "text": "ইন্না-ঝাইয়ান্নাছছামাআদ্দুনইয়া-বিঝীনাতিনিল কাওয়া-কিব।",
        "numberInSurah": 6,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3795,
        "text": "ওয়া হিফজাম মিন কুল্লি শাইতা-নিম মা-রিদ।",
        "numberInSurah": 7,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3796,
        "text": "লা ইয়াছছাম্মা‘ঊনা ইলাল মালাইল আ‘লা-ওয়া ইউকযাফূনা মিন কুল্লি জা-নিব।",
        "numberInSurah": 8,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3797,
        "text": "দুহূ রাও ওয়ালাহুম ‘আযা-বুন ওয়া-সিব।",
        "numberInSurah": 9,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3798,
        "text": "ইল্লা-মান খাতিফাল খাতফাতা ফাআতবা‘আহূশিহা-বুন ছাকিব।",
        "numberInSurah": 10,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3799,
        "text": "ফাছতাফতিহিম আহুম আশাদ্দুখাল কান আম্মান খালাকনা- ইন্না-খালাকনা-হুম মিন তীনিল লা-ঝিব।",
        "numberInSurah": 11,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3800,
        "text": "বাল ‘আজিবতা ওয়া ইয়াছখারূন।",
        "numberInSurah": 12,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3801,
        "text": "ওয়া ইযা-যুক্কিরূলা-ইয়াযকুরূন।",
        "numberInSurah": 13,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3802,
        "text": "ওয়া ইযা-রাআও আ-য়াতাই ইয়াছতাছখিরূন।",
        "numberInSurah": 14,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3803,
        "text": "ওয়া কা-লূ ইন হা-যাইল্লা-ছিহরু মুবীন।",
        "numberInSurah": 15,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3804,
        "text": "আইযা-মিতনা-ওয়া কুন্না-তুরাবাওঁ ওয়া ‘ইজা-মান আইন্না-লামাব‘ঊছূন।",
        "numberInSurah": 16,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3805,
        "text": "আওয়াআ-বাউনাল আওওয়ালূন।",
        "numberInSurah": 17,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3806,
        "text": "কুল না‘আম ওয়া আনতুম দা-খিরূন।",
        "numberInSurah": 18,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3807,
        "text": "ফাইন্নামা-হিয়া ঝাজরাতুওঁ ওয়া-হিদাতুন ফাইযা-হুম ইয়ানজু রূন।",
        "numberInSurah": 19,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3808,
        "text": "ওয়া-কা-লূইয়া-ওয়াইলানা-হা-যা-ইয়াওমুদ্দীন।",
        "numberInSurah": 20,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3809,
        "text": "হা-যা-ইয়াওমুল ফাসলিল্লাযী কুনতুম বিহী তুকাযযিবূন।",
        "numberInSurah": 21,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 386,
        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3810,
        "text": "উহশুরুল্লাযীনা জালামূওয়া আঝওয়া-জাহুম ওয়ামা-কা-নূইয়া‘বুদূন।",
        "numberInSurah": 22,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3811,
        "text": "মিন দূনিল্লা-হি ফাহদূহুম ইলা-সিরা-তিল জাহীম।",
        "numberInSurah": 23,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3812,
        "text": "ওয়াকিফূহুম ইন্নাহুম মাছঊলূন।",
        "numberInSurah": 24,
        "juz": 23,
        "manzil": 6,
        "page": 446,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3813,
        "text": "মা-লাকুম লা-তানা-সারূন।",
        "numberInSurah": 25,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3814,
        "text": "বাল হুমুল ইয়াওমা মুছতাছলিমূন।",
        "numberInSurah": 26,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3815,
        "text": "ওয়া আকবালা বা‘দুহুম ‘আলা-বা‘দিইঁ ইয়াতাছাআলূন।",
        "numberInSurah": 27,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3816,
        "text": "কা-লূইন্নাকুম কুনতুম তা’তূনানা-‘আনিল ইয়ামীন।",
        "numberInSurah": 28,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3817,
        "text": "কা-লূবাল লাম তাকূনূমু’মিনীন।",
        "numberInSurah": 29,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3818,
        "text": "ওয়ামা-কা-না লানা-‘আলাইকুম মিন ছুলতা-নিন বাল কুনতুম কাওমান তা-গীন।",
        "numberInSurah": 30,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3819,
        "text": "ফাহাক্কা ‘আলাইনা-কাওলুরাব্বিনা  ইনা-লাযাইকূন।",
        "numberInSurah": 31,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3820,
        "text": "ফাআগওয়াইনা-কুম ইন্না-কুন্না-গা-বীন।",
        "numberInSurah": 32,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3821,
        "text": "ফাইন্নাহুম ইয়াওমাইযিন ফিল ‘আযা-বি মুশতারিকূন।",
        "numberInSurah": 33,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3822,
        "text": "ইন্না-কাযা-লিকা নাফ‘আলুবিলমুজরিমীন।",
        "numberInSurah": 34,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3823,
        "text": "ইন্নাহুম কা-নূইযা-কীলা লাহুম লাইলা-হা ইল্লাল্লা-হু ইয়াছতাকবিরূন।",
        "numberInSurah": 35,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3824,
        "text": "ওয়া ইয়াকূলূনা আইন্না-লাতা-রিকূআ-লিহাতিনা-লিশা-‘ইরিম মাজনূন।",
        "numberInSurah": 36,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3825,
        "text": "বাল জাআ বিলহাক্কিওয়া সাদ্দাকাল মুরছালীন।",
        "numberInSurah": 37,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3826,
        "text": "ইন্নাকুম লাযাইকু ল ‘আযা-বিল আলীম।",
        "numberInSurah": 38,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3827,
        "text": "ওয়া মা-তুজঝাওনা ইল্লা-মা-কুনতুম তা‘মালূন।",
        "numberInSurah": 39,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3828,
        "text": "ইল্লা-‘ইবাদাল্লা-হিল মুখলাসীন।",
        "numberInSurah": 40,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3829,
        "text": "উলাইকা লাহুম রিঝকুম মা‘লূম।",
        "numberInSurah": 41,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3830,
        "text": "ফাওয়া-কিহু ওয়া হুম মুকরামূন।",
        "numberInSurah": 42,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3831,
        "text": "ফী জান্না-তিন না‘ঈম।",
        "numberInSurah": 43,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3832,
        "text": "‘আলা-ছুরুরিম মুতাকা-বিলীন।",
        "numberInSurah": 44,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3833,
        "text": "ইউতা-ফু‘আলাইহিম বিকা’ছিম মিম মা‘ঈন।",
        "numberInSurah": 45,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3834,
        "text": "বাইদাআ লাযযাতিল লিশশা-রিবীন।",
        "numberInSurah": 46,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3835,
        "text": "লা-ফীহা-গাওলুওঁ ওয়ালা-হুম ‘আনহা-ইউনঝাফূন।",
        "numberInSurah": 47,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3836,
        "text": "ওয়া ‘ইনদাহুম কা-সিরা-তুত্তারফি ‘ঈন।",
        "numberInSurah": 48,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3837,
        "text": "কাআন্নাহুন্না বাইদুম মাকনূন।",
        "numberInSurah": 49,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3838,
        "text": "ফাআকবালা বা‘দুহুম ‘আলা-বা‘দিইঁ ইয়াতাছাআলূন।",
        "numberInSurah": 50,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3839,
        "text": "কা-লা কা-ইলুম মিনহুম ইন্নী কা-না লী কারীন।",
        "numberInSurah": 51,
        "juz": 23,
        "manzil": 6,
        "page": 447,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3840,
        "text": "ইয়াকূ লুআইন্নাকা লামিনাল মুসাদ্দিকীন।",
        "numberInSurah": 52,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3841,
        "text": "আইযা-মিতনা-ওয়া কুন্না-তুরা-বাওঁ ওয়া ‘ইজা-মান আইন্না-লামাদীনূন।",
        "numberInSurah": 53,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3842,
        "text": "কা-লা হাল আনতুম মুত্তালি‘ঊন।",
        "numberInSurah": 54,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3843,
        "text": "ফাত্তালা‘আ ফারাআ-হু ফী ছাওয়াইল জাহীম।",
        "numberInSurah": 55,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3844,
        "text": "কা-লা তাল্লা-হি ইন কিত্তা লাতুরদীন।",
        "numberInSurah": 56,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3845,
        "text": "ওয়া লাওলা-নি‘মাতুরাববী লাকুনতুমিনাল মুহদারীন।",
        "numberInSurah": 57,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3846,
        "text": "আফামা-নাহনু বিমাইয়িতীন।",
        "numberInSurah": 58,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3847,
        "text": "ইল্লা-মাওতাতানাল ঊলা-ওয়ামা-নাহনুবিমু‘আযযাবীন।",
        "numberInSurah": 59,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3848,
        "text": "ইন্না হা-যা-লাহুওয়াল ফাওঝুল ‘আজীম।",
        "numberInSurah": 60,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3849,
        "text": "লিমিছলি হা-যা-ফাল ইয়া‘মালিল ‘আ-মিলূন।",
        "numberInSurah": 61,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3850,
        "text": "আযা-লিকা খাইরুন নুঝুলান আম শাজারাতুঝ ঝাককূম।",
        "numberInSurah": 62,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3851,
        "text": "ইন্না-জা‘আলনা-হা-ফিতনাতাল লিজ্জা-লিমীন।",
        "numberInSurah": 63,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3852,
        "text": "ইন্নাহা-শাজারাতুন তাখ রুজুফীআসলিল জাহীম।",
        "numberInSurah": 64,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3853,
        "text": "তাল‘উহা-কাআন্নাহূরুঊছুশশায়া-তীন।",
        "numberInSurah": 65,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3854,
        "text": "ফাইন্নাহুম লাআ-কিলূনা মিনহা-ফামা-লিঊনা মিনহাল বুতূন।",
        "numberInSurah": 66,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3855,
        "text": "ছু ম্মা ইন্না লাহুম ‘আলাইহা- লাশাওবাম মিন হামীম।",
        "numberInSurah": 67,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3856,
        "text": "ছু ম্মা ইন্না মারজি‘আহুম লা ইলাল জাহীম।",
        "numberInSurah": 68,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3857,
        "text": "ইন্নাহুম আলফাও আ-বাআহুম দাল্লীন।",
        "numberInSurah": 69,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3858,
        "text": "ফাহুম ‘আলাআ-ছা-রিহিম ইউহরা‘ঊন।",
        "numberInSurah": 70,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3859,
        "text": "ওয়া লাকাদ দাল্লা কাবলাহুম আকছারুল আওওয়ালীন।",
        "numberInSurah": 71,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3860,
        "text": "ওয়া লাকাদ আরছালনা-ফীহিম মুনযিরীন।",
        "numberInSurah": 72,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3861,
        "text": "ফানজু র কাইফা কা-না ‘আ-কিবাতুল মুনযারীন।",
        "numberInSurah": 73,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3862,
        "text": "ইল্লা-‘ইবা-দাল্লা-হিল মুখলাসীন।",
        "numberInSurah": 74,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 387,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3863,
        "text": "ওয়ালা কাদ না-দা-না-নূহুন ফালানি‘মাল মুজীবূন।",
        "numberInSurah": 75,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 388,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3864,
        "text": "ওয়া নাজ্জাইনা-হু ওয়া আহলাহূমিনাল কারবিল ‘আজীম।",
        "numberInSurah": 76,
        "juz": 23,
        "manzil": 6,
        "page": 448,
        "ruku": 388,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3865,
        "text": "ওয়া জা‘আলনা- যুররিইইয়াতাহূহুমুল বাকীন।",
        "numberInSurah": 77,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3866,
        "text": "ওয়া তারাকনা-‘আলাইহি ফিল আ-খিরীন।",
        "numberInSurah": 78,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3867,
        "text": "ছালা-মুন ‘আলা-নূহিন ফিল ‘আ-লামীন।",
        "numberInSurah": 79,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3868,
        "text": "ইন্না-কাযা-লিকা নাজঝিল মুহছিনীন।",
        "numberInSurah": 80,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3869,
        "text": "ইন্নাহূমিন ‘ইবা-দিনাল মু’মিনীন।",
        "numberInSurah": 81,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3870,
        "text": "ছু ম্মা আগরাকনাল আ-খারীন।",
        "numberInSurah": 82,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 179,
        "sajda": false
      },
      {
        "number": 3871,
        "text": "ওয়া ইন্না মিন শী‘আতিহী লাইবরা-হীম।",
        "numberInSurah": 83,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3872,
        "text": "ইযজাআ রাব্বাহূবিকালবিন ছালীম।",
        "numberInSurah": 84,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3873,
        "text": "ইযকা-লা লিআবীহি ওয়া কাওমিহী মা-যা-তা‘বুদূন।",
        "numberInSurah": 85,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3874,
        "text": "আইফকান আ-লিহাতান দূনাল্লা-হি তুরীদূন।",
        "numberInSurah": 86,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3875,
        "text": "ফমা-জান্নুকুম বিরাব্বিল ‘আ-লামীন।",
        "numberInSurah": 87,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3876,
        "text": "ফানাজারা নাজরাতান ফিননুজূম।",
        "numberInSurah": 88,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3877,
        "text": "ফাকা-লা ইন্নী ছাকীম।",
        "numberInSurah": 89,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3878,
        "text": "ফাতওয়াল্লাও ‘আনহু মুদবিরীন।",
        "numberInSurah": 90,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3879,
        "text": "ফারা-গা ইলাআ-লিহাতিহিম ফাকা-লা আলা-তা’কুলূন।",
        "numberInSurah": 91,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3880,
        "text": "মা-লাকুম লা-তানতিকূন।",
        "numberInSurah": 92,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3881,
        "text": "ফারা-গা ‘আলাইহিম দারবাম বিলইয়ামীন।",
        "numberInSurah": 93,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3882,
        "text": "ফাআকবালূইলাইহি ইয়াঝিফফূন।",
        "numberInSurah": 94,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3883,
        "text": "কা-লা আতা‘বুদূনা মা-তানহিতূন।",
        "numberInSurah": 95,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3884,
        "text": "ওয়াল্লা-হু খালাকাকুম ওয়ামা-তা‘মালূন।",
        "numberInSurah": 96,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3885,
        "text": "কা-লুবনূলাহূবুনইয়া-নান ফাআলকূহু ফিল জাহীম।",
        "numberInSurah": 97,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3886,
        "text": "ফাআরাদূবিহী কাইদান ফাজা‘আলনা-হুমুল আছফালীন।",
        "numberInSurah": 98,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3887,
        "text": "ওয়া কা-লা ইন্নী যা-হিবুন ইলা-রাববী ছাইয়াহদীন।",
        "numberInSurah": 99,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3888,
        "text": "রাব্বি হাবলী মিনাসসা-লিহীন।",
        "numberInSurah": 100,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3889,
        "text": "ফাবাশশারনা-হু বিগুলা-মিন হালীম।",
        "numberInSurah": 101,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3890,
        "text": "ফালাম্মা-বালাগা মা‘আহুছ ছা‘ইয়া কা-লা ইয়া-বুনাইইয়া ইন্নীআরা-ফিল মানা-মি আন্নী আযবাহুকা ফানজুর মা-যা-তারা- কা-লা ইয়াআবাতিফ‘আল মা-তু’মারু ছাতাজিদুনীইন শা-আল্লা-হু মিনাসসা-বিরীন।",
        "numberInSurah": 102,
        "juz": 23,
        "manzil": 6,
        "page": 449,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3891,
        "text": "ফালাম্মাআছলামা-ওয়া তাল্লাহূলিলজাবীন।",
        "numberInSurah": 103,
        "juz": 23,
        "manzil": 6,
        "page": 450,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3892,
        "text": "ওয়া না-দাইনা-হু আইঁ ইয়াইবরা-হীম।",
        "numberInSurah": 104,
        "juz": 23,
        "manzil": 6,
        "page": 450,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3893,
        "text": "কাদ সাদ্দাকতার রু’ইয়া- ইন্না-কাযা-লিকা নাজঝিল মুহছিনীন।",
        "numberInSurah": 105,
        "juz": 23,
        "manzil": 6,
        "page": 450,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3894,
        "text": "ইন্না হা-যা-লাহুওয়াল বালাউল মুবীন।",
        "numberInSurah": 106,
        "juz": 23,
        "manzil": 6,
        "page": 450,
        "ruku": 388,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3895,
        "text": "ওয়া ফাদাইনা-হু বিযিবহিন ‘আজীম।",
