        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 389,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3916,
        "text": "ইল্লা-‘ইবাদাল্লা-হিল মুখলাসীন।",
