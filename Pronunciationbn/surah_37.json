        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 389,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3920,
        "text": "ইন্নাহু মিন ‘ইবা-দিনাল মু’মিনীন।",
