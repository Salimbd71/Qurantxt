        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 389,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3917,
        "text": "ওয়া তারাকনা-‘আলাইহি ফিল আ-খিরীন।",
        "numberInSurah": 129,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 389,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3918,
        "text": "ছালা-মুন ‘আলাইলইয়া-ছীন।",
