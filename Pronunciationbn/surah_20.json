        "numberInSurah": 28,
        "juz": 16,
        "manzil": 4,
        "page": 313,
        "ruku": 271,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
        "number": 2377,
        "text": "ওয়াজ‘আললী ওয়াঝীরাম মিন আহলী।",
        "numberInSurah": 29,
        "juz": 16,
        "manzil": 4,
        "page": 313,
        "ruku": 271,
        "hizbQuarter": 125,
        "sajda": false
      },
      {
