        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 390,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3930,
        "text": "ফালতাকামাহুল হূতুওয়া হুওয়া মুলীম।",
        "numberInSurah": 142,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 390,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3931,
        "text": "ফালাওলাআন্নাহূকা-না মিনাল মুছাব্বিহীন।",
