        "numberInSurah": 149,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3938,
        "text": "আম খালাকনাল মালাইকাতা ইনা-ছাওঁ ওয়া হুম শা-হিদূন।",
        "numberInSurah": 150,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3939,
        "text": "আলাইন্নাহুম মিন ইফকিহিম লাইয়াকূলূন।",
        "numberInSurah": 151,
        "juz": 23,
        "manzil": 6,
        "page": 451,
        "ruku": 390,
        "hizbQuarter": 181,
        "sajda": false
      },
      {
        "number": 3940,
        "text": "ওয়ালাদাল্লা-হু ওয়া ইন্নাহুম লাকা-যিবূন।",
